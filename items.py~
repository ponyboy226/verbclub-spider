# Define here the models for your scraped items
#
# See documentation in:
# http://doc.scrapy.org/topics/items.html

from scrapy.item import Item, Field

class DmozItem(Item):
    latitude = Field()
    longitude = Field()
    name = Field()
    location = Field()
    dificulty = Field()
    trail_length = Field()
    title = Field()
    monday = Field()
    tuesday = Field()
    wednesday = Field()
    thursday = Field()
    friday = Field()
    saturday = Field()
    sunday = Field()
    pass
